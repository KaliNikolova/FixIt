# Backend environment variables template
# Copy this file to .env in the backend directory
# Update with your actual API keys before running

# Gemini API Keys (obtain from Google Cloud console)
gemini_api_key=your_gemini_api_key_here
gemini_image_api_key=your_gemini_image_api_key_here
gemini_search_api_key=your_gemini_search_api_key_here

# Database URL (optional, defaults to SQLite)
# For local dev: sqlite:///./fixit.db (default)
# For production: postgresql://user:password@host/dbname
database_url=sqlite:///./fixit.db
